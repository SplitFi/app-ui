{
  "extends": "@splitfi/typescript-config/nextjs.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "#/*": [
        "./types/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "next.config.js",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
  ],
  "exclude": ["./out/**/*", "./node_modules/**/*", "cypress/**/*.ts", "./types/**/typechain/**/*.ts"],
}
